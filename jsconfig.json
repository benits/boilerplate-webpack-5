{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@src/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@assets/*": [
                "src/assets*"
            ],
            "services/*": [
                "src/services/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
        }
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}